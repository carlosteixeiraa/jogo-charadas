<?xml version='1.0' encoding='utf-8'?>
<widget id="xyz.carlosteixeira.jogocharadas" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Jogo Charadas</name>
    <description>
        Por escrever xD xD
    </description>
    <author email="hello@carlosteixeira.xyz" href="https://carlosteixeira.xyz">
        Carlos Teixeira
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="www/img/logo.png" />
    <preference name="SplashScreenDelay" value="0" />
    <preference name="orientation" value="landscape" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
